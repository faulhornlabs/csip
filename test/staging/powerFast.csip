# stage

import Prelude

data Int : Ty where
  One : Int
  Mul : Int -> Int -> Int
  Add : Int -> Int -> Int

instance Num (Code Int) where
  a + b = Add a b

sqr = \a -> do
  b := a
  Mul b b

(^) : Int -> Nat -> Int
a ^ 0 = One
a ^ 1 = a
a ^ n | even n = sqr (a ^ div n 2)
a ^ Succ n = do
  c := a
  Mul c (c ^ n)

\a b -> ((a + b)^100 + b)^80
